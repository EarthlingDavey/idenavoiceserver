{
  "name": "idenavoiceserver-syncer",
  "version": "0.0.1",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "start-dev": "./node_modules/.bin/nodemon --watch src --ext js,graphql --exec babel-node  src/index.js",
    "build": "babel src -d build && shx cp .env build && shx cp src/schema.graphql build",
    "start": "npm run build && node build/index.js"
  },
  "author": "William Lyon",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^7.0.0",
    "ethereumjs-util": "^7.0.1",
    "express": "^4.17.1",
    "git-repo-info": "^2.1.1",
    "json-parse-safe": "^2.0.0",
    "neo4j-driver": "^4.0.2",
    "node-fetch": "^2.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "nodemon": "^1.19.1",
    "shx": "^0.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,md,graphql": [
      "prettier --write",
      "git add"
    ]
  }
}
